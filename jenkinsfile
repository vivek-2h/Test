pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('GoodBye') {
            steps {
                echo 'GoodBye World'
            }
        }
        stage('pwsh Hello') {
            steps {
                powershell label: '', script: 'Write-Output "Hello PowerShell!\''
            }
        }
        
    }
}
